<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="loginform" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label fx:id="loginlabel" layoutX="248.0" layoutY="65.0" text="教师登录">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Label fx:id="usernameLable" layoutX="162.0" layoutY="143.0" text="账号：">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Label fx:id="passwordLable" layoutX="162.0" layoutY="207.0" text="密码：">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextfield" layoutX="251.0" layoutY="145.0" />
      <PasswordField fx:id="passwordField" layoutX="251.0" layoutY="209.0" />
      <Button fx:id="loginButton" layoutX="209.0" layoutY="277.0" mnemonicParsing="false" onAction="#login" prefHeight="30.0" prefWidth="78.0" text="登录" styleClass="button-common"/>
      <Button fx:id="clearButton" layoutX="340.0" layoutY="277.0" mnemonicParsing="false" onAction="#clear" prefHeight="30.0" prefWidth="78.0" text="清除" styleClass="button-common"/>
   </children>
</AnchorPane>
